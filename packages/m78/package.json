{
  "name": "m78",
  "version": "1.0.0-beta.100",
  "description": "components, hooks, utils, part of m78 project",
  "keywords": [
    "react",
    "react-components",
    "ui-components",
    "typescript",
    "react-toolkits"
  ],
  "scripts": {
    "test": "jest",
    "lint:prettier": "prettier ./src ./test --write --no-error-on-unmatched-pattern",
    "lint:script": "eslint ./src ./test --ext .js,.jsx,.ts,.tsx,.vue --fix",
    "lint": "npm run lint:script && npm run lint:prettier",
    "build": "mbt build",
    "pub": "pnpm build && pnpm publish --access public --registry https://registry.npmjs.org --no-git-checks",
    "example-hello": "mbt example hello",
    "example-seed": "mbt example seed",
    "example-i18n": "mbt example i18n",
    "example-spin": "mbt example spin",
    "example-button": "mbt example button"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@lxjx/click-effect": "^1.0.1",
    "@m78/hooks": "workspace:^",
    "@m78/seed": "workspace:^",
    "@m78/style": "workspace:^",
    "@m78/utils": "workspace:^",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "i18next": "^22.0.5",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0",
    "react-spring": "^9.5.5",
    "use-sync-external-store": "^1.2.0"
  },
  "author": {
    "name": "Link",
    "email": "1073830130@qq.com"
  },
  "license": "MIT",
  "files": [
    "**"
  ],
  "type": "module",
  "typings": "./",
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": true
  },
  "devDependencies": {
    "@types/use-sync-external-store": "^0.0.3"
  }
}
