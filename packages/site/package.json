{
  "private": true,
  "name": "dumi-app",
  "scripts": {
    "start": "dumi dev",
    "build": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\""
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@m78/hooks": "workspace:^",
    "@m78/utils": "workspace:^",
    "@umijs/plugin-sass": "^1.1.1",
    "dumi": "^1.1.47",
    "lint-staged": "^10.0.7",
    "yorkie": "^2.0.0",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "^18.0.20",
      "@types/react-dom": "^18.0.6",
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  }
}
